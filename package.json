{
  "name": "sass-starter",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": true,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blitz-guard/core": "0.3.1",
    "@prisma/client": "2.26.0",
    "@stripe/stripe-js": "1.16.0",
    "blitz": "0.38.5",
    "envsafe": "2.0.3",
    "final-form": "4.20.2",
    "react": "alpha",
    "react-dom": "alpha",
    "react-final-form": "6.5.3",
    "slugify": "1.5.3",
    "stripe": "8.161.0",
    "zod": "3.5.1"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.14",
    "eslint": "7.30.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2",
    "prettier-plugin-prisma": "0.13.2",
    "pretty-quick": "3.1.1",
    "preview-email": "3.0.4",
    "prisma": "2.26.0",
    "typescript": "~4.3"
  },
  "private": true
}
